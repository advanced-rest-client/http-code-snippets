{
  "name": "http-code-snippets",
  "version": "0.0.1",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Code snippets to display a snippets of making a HTTP request",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "http-code-snippets.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "list": "node_modules/.bin/polymer lint http-code-snippets.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"http-code-snippets:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/http-code-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/http-code-snippets/issues",
    "email": "arc@mulesoft.com"
  }
}