{
  "description": "Code snippets to display a snippets of making a HTTP request",
  "keywords": [
    "web-components",
    "polymer"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/http-code-snippets.git"
  },
  "name": "@advanced-rest-client/http-code-snippets",
  "version": "3.0.0-preview.1",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polymer/gen-typescript-declarations": "^1.6.1",
    "@polymer/iron-component-page": "^4.0.0",
    "@polymer/iron-demo-helpers": "^3.0.0",
    "@polymer/iron-test-helpers": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "@polymer/test-fixture": "^4.0.2",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sinon": "^7.2.3",
    "wct-mocha": "^1.0.0"
  },
  "main": "http-code-snippets.js",
  "scripts": {
    "lint": "polymer lint http-code-snippets.html c-curl-http-snippet.html curl-http-snippet.html fetch-js-http-snippet.html http-code-snippets-style.html http-code-snippets.html javascript-http-snippets.html node-http-snippet.html python-27-http-snippet.html python-31-http-snippet.html python-http-snippets.html raw-http-snippet.html requests-python-http-snippet.html xhr-http-snippet.html",
    "test-sauce": "polymer test --plugin sauce --job-name \"http-code-snippets:local-test\"",
    "test": "polymer test --plugin local",
    "update-types": "gen-typescript-declarations --deleteExisting --outDir ."
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/http-code-snippets/issues",
    "email": "arc@mulesoft.com"
  },
  "authors": [
    "Pawel Psztyc",
    "The Advanced REST client authors <arc@mulesoft.com>"
  ],
  "contributors": [
    "Pawel Psztyc",
    "The Advanced REST client authors <arc@mulesoft.com>"
  ],
  "dependencies": {
    "@advanced-rest-client/arc-icons": "^3.0.0-preview.1",
    "@polymer/iron-flex-layout": "^3.0.0",
    "@polymer/iron-icon": "^3.0.0",
    "@polymer/iron-pages": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-tabs": "^3.0.0",
    "@polymer/polymer": "^3.0.0",
    "@polymer/prism-element": "^3.0.0"
  }
